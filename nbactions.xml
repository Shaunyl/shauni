<?xml version="1.0" encoding="UTF-8"?>
<actions>
        <action>
            <actionName>run</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
            </goals>
            <properties>
                <exec.args>-classpath %classpath com.fil.shauni.Main exp -format=tab -tables=sys.dba_source,sys.dba_registry -parallel=3 -cluster=2 -directory=out -filename=%t-%d_[%n-%w%u]</exec.args>
                <exec.executable>java</exec.executable>
            </properties>
        </action>
        <action>
            <actionName>debug</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
            </goals>
            <properties>
                <exec.args>-Xdebug -Xrunjdwp:transport=dt_socket,server=n,address=${jpda.address} -classpath %classpath com.fil.shauni.Main exp -format=tab -tables=sys.dba_source,sys.dba_registry -parallel=3 -cluster=2 -directory=out -filename=%t-%d_[%n-%w%u]</exec.args>
                <exec.executable>java</exec.executable>
                <jpda.listen>true</jpda.listen>
            </properties>
        </action>
        <action>
            <actionName>profile</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
            </goals>
            <properties>
                <exec.args>-classpath %classpath com.fil.shauni.Main exp -format=tab -tables=sys.dba_source,sys.dba_registry -parallel=3 -cluster=2 -directory=out -filename=%t-%d_[%n-%w%u]</exec.args>
                <exec.executable>java</exec.executable>
            </properties>
        </action>
    </actions>
